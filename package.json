{
  "main": "./src/wa4e-v2.js",
  "module": "./src/wa4e-v2.js",
  "devDependencies": {
    "@eslint/eslintrc": "^1.4.0",
    "@open-wc/building-rollup": "^2.2.1",
    "@open-wc/dev-server-hmr": "^0.1.3",
    "@open-wc/eslint-config": "^7.0.0",
    "@open-wc/testing": "^3.1.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/test-runner": "^0.13.31",
    "deepmerge": "^4.2.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.mjs,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.mjs,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "test": "web-test-runner",
    "test:coverage": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch",
    "build": "rimraf dist && npx rollup -c rollup.config.js",
    "start:build": "npm run build && es-dev-server --root-dir dist --app-index src/index.html --open --compatibility none",
    "start": "web-dev-server"
  },
  "name": "wa4e-v2",
  "version": "0.0.0",
  "description": "Webcomponent wa4e-v2 following open-wc recommendations",
  "author": "we4e-v2",
  "license": "",
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@popperjs/core": "^2.11.6",
    "@sideway/address": "^5.0.0",
    "@stdlib/array-linspace": "^0.0.7",
    "@stdlib/array-logspace": "^0.0.7",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.11.2",
    "core-js": "^3.26.1",
    "file-saver-es": "^2.0.5",
    "joi": "^17.7.0",
    "jsonexport": "^3.2.0",
    "lit": "2.2.5",
    "load-json-file": "^7.0.1",
    "lodash-es": "^4.17.21",
    "masonry-layout": "^4.2.2",
    "mathjs": "^10.6.4",
    "package.json": "^0.0.0",
    "plotly.js-dist-min": "^2.16.5",
    "rfs": "^9.0.6",
    "source-map": "0.7.4",
    "stream": "^0.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.19"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.mjs": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
